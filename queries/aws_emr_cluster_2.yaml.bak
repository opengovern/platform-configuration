ID: aws_emr_cluster_2
Title: "Find AWS EMR Clusters That Do Not Auto-Terminate"
Description: "Allows users to query AWS Elastic MapReduce Cluster data."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      name,
      cluster_arn,
      auto_terminate
    from
      aws_emr_cluster
    where
      not auto_terminate;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - Elastic MapReduce
