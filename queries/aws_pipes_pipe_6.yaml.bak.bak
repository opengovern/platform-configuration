ID: aws_pipes_pipe_6
Title: "List AWS Pipes and Their Associated IAM Roles"
Description: "Allows users to query AWS Pipes to obtain detailed information about individual pipes."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      p.name,
      r.arn as role_arn,
      r.role_id,
      r.permissions_boundary_arn,
      r.role_last_used_region,
      r.inline_policies,
      r.assume_role_policy
    from
      aws_pipes_pipe as p,
      aws_iam_role as r
    where
      p.role_arn = r.arn;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_identity_security:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - AWS Pipes
