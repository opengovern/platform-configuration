ID: aws_dms_endpoint_3
Title: "List all AWS DMS Endpoints for connection details"
Description: "Query AWS DMS Endpoints to retrieve connection information for source or target databases in database migration activities."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      endpoint_identifier,
      arn,
      engine_name,
      instance_create_time,
      my_sql_settings
    from
      aws_dms_endpoint
    where
      engine_name = 'mysql';
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_data_security:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - DMS
