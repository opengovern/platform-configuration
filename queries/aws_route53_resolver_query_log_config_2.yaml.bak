ID: aws_route53_resolver_query_log_config_2
Title: "List all AWS Route 53 Resolver Query Log Configurations"
Description: "Allows users to query AWS Route 53 Resolver Query Log Configurations."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      name,
      id,
      arn,
      creation_time,
      share_status,
      status,
      destination_arn
    from
      aws_route53_resolver_query_log_config
    where
      owner_id <> account_id;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - Route 53
  cloud_traffic_analytics:
  - "true"
