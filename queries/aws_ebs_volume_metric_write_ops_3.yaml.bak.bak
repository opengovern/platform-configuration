ID: aws_ebs_volume_metric_write_ops_3
Title: "Find AWS EBS volume write ops metrics"
Description: "Allows users to query AWS Elastic Block Store (EBS) volume write operations metrics."
IntegrationTypeName:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      volume_id,
      timestamp,
      minimum,
      maximum,
      average,
      sum,
      sample_count
    from
      aws_ebs_volume_metric_write_ops
    where
      maximum > 8000
    order by
      volume_id,
      timestamp;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - Elastic Block Store
