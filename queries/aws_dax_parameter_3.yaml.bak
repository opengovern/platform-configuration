ID: aws_dax_parameter_3
Title: "List All DAX Parameter Groups and Configurations in AWS"
Description: "Allows users to query AWS DAX Parameter Groups to retrieve information about their configuration settings."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      parameter_name,
      parameter_group_name,
      parameter_value,
      data_type,
      parameter_type,
      is_modifiable
    from
      aws_dax_parameter
    where
      is_modifiable = 'TRUE';
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - DAX
