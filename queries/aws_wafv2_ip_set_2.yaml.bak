ID: aws_wafv2_ip_set_2
Title: "List all AWS WAFv2 IPSet Information with Metadata"
Description: "Allows users to query AWS WAFv2 IPSet information, including IP addresses, IP address version, and associated metadata."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      name,
      description,
      arn,
      id,
      scope,
      addresses,
      ip_address_version,
      region
    from
      aws_wafv2_ip_set
    where
      scope = 'CLOUDFRONT';
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_network_security:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - WAFv2
