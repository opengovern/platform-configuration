ID: aws_mq_broker_1
Title: "List all AWS MQ Brokers with Key Details"
Description: "Allows users to query AWS MQ Brokers."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      arn,
      broker_name,
      broker_state,
      deployment_mode,
      created,
      host_instance_type,
      engine_type,
      engine_version
      tags
    from
      aws_mq_broker;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - MQ
