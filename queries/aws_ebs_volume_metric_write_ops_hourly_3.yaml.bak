ID: aws_ebs_volume_metric_write_ops_hourly_3
Title: "List all AWS EBS Volume Metrics on Hourly Write Ops"
Description: "Allows users to query AWS EBS Volume Metrics on hourly write operations."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      volume_id,
      timestamp,
      minimum,
      maximum,
      average,
      sum,
      sample_count
    from
      aws_ebs_volume_metric_write_ops_hourly
    where
      maximum > 8000
    order by
      volume_id,
      timestamp;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - AWS EBS
