ID: aws_codebuild_project_1
Title: "Find AWS CodeBuild Projects with Comprehensive Details"
Description: "Allows users to query AWS CodeBuild Projects and retrieve comprehensive information about each project."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      name,
      description,
      encryption_key,
      concurrent_build_limit,
      source_version,
      service_role,
      created,
      last_modified,
      region
    from
      aws_codebuild_project;
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_asset_management:
  - "true"
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - CodeBuild
