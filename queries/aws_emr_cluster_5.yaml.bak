ID: aws_emr_cluster_5
Title: "Find AWS Elastic MapReduce Cluster Data"
Description: "Allows users to query AWS Elastic MapReduce Cluster data."
Connector:
- aws
Query:
  Engine: CloudQL-v0.0.1
  QueryToExecute: |-
    select
      name,
      cluster_arn,
      log_uri
    from
      aws_emr_cluster
    where
      log_uri is null
  PrimaryTable: null
  ListOfTables: []
  Parameters: []
Tags:
  cloud_ops:
  - "true"
  cloud_provider:
  - aws
  cloud_service:
  - Elastic MapReduce
